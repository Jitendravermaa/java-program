
import java.lang.*;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        // YOUR CODE GOES HERE
     Scanner Sc =new Scanner(System.in); 
        int a=Sc.nextInt();
        int b=Sc.nextInt();
       System.out.print((a+b) + " ");
       System.out.print((a*b) + " ");
       System.out.print((a-b) + " ");
       System.out.print((a/b)+"");  

   
    }
