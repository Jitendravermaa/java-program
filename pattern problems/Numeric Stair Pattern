import java.lang.*;
import java.util.Scanner;
public class Main{
   public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
          int N=sc.nextInt();
    for(int i=1;i<=N;i++){
    for(int cnt=1;cnt<=i;cnt++){
     System.out.print(cnt);
     if (cnt < i)
      System.out.print(" ");
     }  
    System.out.println(); 
     }   
  }
}
